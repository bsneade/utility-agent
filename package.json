{
  "name": "utility-agent",
  "version": "1.0.0",
  "description": "Utility Agent AI",
  "author": "Brad Sneade",
  "license": "MIT",
  "scripts": {
    "test": "jest --forceExit",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "pretest": "npm run lint"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "Qualifier((\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "json"
    ]
  },
  "dependencies": {
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/winston": "^2.3.6",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.3",
    "typescript": "^2.5.3"
  }
}
